        "@material-ui/lab": "^4.0.0-alpha.46",
        "@mui/x-data-grid": "^4.0.2",
        "apexcharts": "^3.26.2",
        "d3-scale": "^3.2.1",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.32",
        "react": "^17.0.1",
        "react-apexcharts": "^1.3.6",
        "react-dom": "^17.0.1",
        "react-json-view": "^1.21.3",
        "react-moment": "^0.9.7",
        "react-router": "^3.2.5",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "chokidar": "^3.5.3",
