{
    "name": "trackmania-records",
    "version": "2.1.0",
    "author": "NeKz",
    "scripts": {
        "predeploy": "npm run build",
        "start": "react-scripts start",
        "build": "react-scripts build && cp build/index.html build/404.html",
        "test": "react-scripts test --env=jsdom",
